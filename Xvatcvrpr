                local cout = 0 
                for i , v in next , option do 
                    cout =cout + 1 
                    if count == 0 then
                        FrameSize = 30 
                    end
                    if cout == 1 then
                        FrameSize = 75
                    elseif cout == 2 then
                        FrameSize = 110
                    elseif cout == 3 then
                        FrameSize = 150
                    elseif cout == 4 then
                        FrameSize = 190
                    elseif cout >= 5  then
                        FrameSize = 190
                    end

