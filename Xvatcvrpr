                                    DropFrame,
                                    TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                                    {Size = UDim2.new(0.9, 0, 0.01, 30)}
                                ):Play()
                            end
                            dog = not dog
                    end)
                
                    ScolDown.CanvasSize = UDim2.new(0,0,0,UIListLayoutlist.AbsoluteContentSize.Y + 10  )  
                    end
                end
            end
            function items:Sliderkp(text,textmatch,min,max,de,callback)
                local SLiderFrame = Instance.new("Frame")

                SLiderFrame.Name = "SLiderFrame"
                SLiderFrame.Parent = PageSecFrame3
                SLiderFrame.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
                SLiderFrame.BorderSizePixel = 0
                SLiderFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
                SLiderFrame.AnchorPoint = Vector2.new(0.5, 0.5)
                SLiderFrame.Size = UDim2.new(0.9, 0, 0.001, 70) -- UDim2.new(0, 213, 0, 35)
                SLiderFrame.BackgroundTransparency  = 1
                SLiderFrame.ClipsDescendants = true   
                conner(5,SLiderFrame,connerSLiderFrame)
                local stork8 = Instance.new("UIStroke")
        
                stork8.Thickness = 2
                stork8.Name = ""
                stork8.Parent = SLiderFrame   
                stork8.LineJoinMode = Enum.LineJoinMode.Round
                stork8.Color = Color3.fromRGB(33, 133, 113)
                stork8.Transparency = 0

                
                local TextSlider = Instance.new("TextLabel")

                TextSlider.Parent = SLiderFrame
                TextSlider.Name = "TextSlider"
                TextSlider.BackgroundColor3 = Color3.fromRGB(249, 53, 139)
                TextSlider.Position = UDim2.new(0.05, 0, 0.1, 0)
                TextSlider.Size = UDim2.new(0.6, 0,0.001, 25)
                TextSlider.Font = Enum.Font.SourceSansSemibold
                TextSlider.Text =  tostring(text)
                TextSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextSlider.TextSize = 16.000
                TextSlider.TextXAlignment = Enum.TextXAlignment.Left
                TextSlider.BackgroundTransparency = 1
                local textSize8 =  game:GetService("TextService"):GetTextSize(TextSlider.Text,TextSlider.TextSize,TextSlider.Font,Vector2.new(999999,999999))
                if textSize8.x > 150 then
                    textSize8 = Vector2.new(150,0)
                end
                TextSlider.Size = UDim2.new(0.6,textSize8.x ,0.001,25)

                local FrameValue = Instance.new("TextButton")

                FrameValue.Name = "FrameValue"
                FrameValue.Parent = SLiderFrame
                FrameValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                FrameValue.BorderSizePixel = 0
                FrameValue.Position = UDim2.new(0.5, 0, 0.7, 0)
                FrameValue.AnchorPoint = Vector2.new(0.5, 0.5)
                FrameValue.Size = UDim2.new(0.9, 0, 0.1, 0) -- UDim2.new(0, 213, 0, 35)
                FrameValue.BackgroundTransparency  = 0
                FrameValue.ClipsDescendants = true   
                FrameValue.Text = ""
                FrameValue.AutoButtonColor = false
                conner(4,FrameValue,connerFrameValue)

                -- local MainValuefuck = Instance.new("TextButton")

                -- MainValuefuck.Name = "FrameValue"
                -- MainValuefuck.Parent = FrameValue
                -- MainValuefuck.BackgroundColor3 = Color3.fromRGB(33, 133, 113)
                -- MainValuefuck.BorderSizePixel = 0
                -- MainValuefuck.Position = UDim2.new(0., 0, 0.5, 0)
