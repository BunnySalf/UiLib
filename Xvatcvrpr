                end

                local dropfunc = {}
                
                function dropfunc:clear()
                    TextLabel_DropText.Text = tostring(text).." : "
                    for i, v in next, ScolDown:GetChildren() do
                        if v:IsA("Frame") then
                            v:Destroy()
                        end
                    end
                    ScolDown.CanvasSize = UDim2.new(0,0,0,UIListLayoutlist.AbsoluteContentSize.Y   )  
                    TweenService:Create(
                        DropFrame,
                        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                        {Size = UDim2.new(0.9, 0, 0.01, 30)}
                    ):Play()
                    dog =  false
                end

                function dropfunc:add(vv)
                    local ListFrame = Instance.new("Frame")

                    ListFrame.Name = "ListFrame"
                    ListFrame.Parent = ScolDown
